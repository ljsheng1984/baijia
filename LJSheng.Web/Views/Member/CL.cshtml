@{
    Layout = null;
}
<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=yes">
    <title>彩链-我的彩链</title>
    <meta name="keywords" content="彩链">
    <meta name="description" content="彩链">
    <meta name="author" content="彩链">
    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
    <link rel="stylesheet" type="text/css" href="/css/style.css" />
</head>

<body>
    <div class="default-header-nav">
        <span>
            <a href="/Member/Index">
                <img src="/images/icon/icon-26.png" />
                </a>
        </span>
        我的彩链
    </div>
    <div class="my-center">
        <div class="my-center-title">
            库存=@ViewBag.Stock
        </div>
        <div class="my-center-content">
            <ul>
                <li>
                    <a href="/Member/MProduct">
                        <img src="/images/icon/icon-32.png" />
                        <span>库存提醒</span>
                    </a>
                    <a href="/Member/Product?BuyPrice=1000.00&Type=4">
                        <img src="/images/icon/icon-33.png" />
                        <span>发货记录</span>
                    </a>
                </li>
                <li class="my-center-singer">
                    <a href="/Member/Bonus?Project=2">
                        <img src="/images/icon/icon-3.png" />
                        <span>彩链分红</span>
                    </a>
                </li>
                <li>
                    <a href="/Member/Product?BuyPrice=1000.00&Type=5">
                        <img src="/images/icon/icon-34.png" />
                        <span>销售记录</span>
                    </a>
                    <a href="/Member/ProductAU?BuyPrice=1000.00">
                        <img src="/images/icon/icon-35.png" />
                        <span>转让商品</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
