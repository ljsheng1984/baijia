</ul>
<div class="user-divider">商城订单</div>
<ul class="admin-menu">
    <li>
        <a href="/Member/ShopOrder?tab=0">
            <div>
                <i class="icon icon-comment"></i>
            </div>
            <p class="tit">全部</p>
        </a>
    </li>
    <li>
        <a href="/Member/ShopOrder?tab=1">
            <div>
                <i class="icon icon-shipment"></i>
            </div>
            <p class="tit">待发货</p>
        </a>
    </li>
    <li>
        <a href="/Member/ShopOrder?tab=2">
            <div>
                <i class="icon icon-goods"></i>
            </div>
            <p class="tit">待收货</p>
        </a>
    </li>
    <li>
        <a href="/Member/ShopOrder?tab=3">
            <div>
                <i class="icon icon-obligations"></i>
            </div>
            <p class="tit">已收货</p>
        </a>
    </li>
</ul>
<div class="user-divider">账户</div>
<ul class="admin-menu">
    <li>
        <a href="/Home/Share?m=@LJSheng.Common.LCookie.GetCK("linjiansheng", "Account")&gid=@LJSheng.Common.LCookie.GetMemberGid()&MID=@ViewBag.MID">
            <div>
                <i class="icon icon-scan-code"></i>
            </div>
            <p class="tit">分享二维码</p>
        </a>
    </li>
    <li>
        <a href="/Member/Integral?tab=1">
            <div>
                <i class="icon icon-bonus"></i>
            </div>
            <p class="tit">我的积分</p>
        </a>
    </li>
    <li>
        <a href="/Member/Withdrawals">
            <div>
                <i class="icon icon-money"></i>
            </div>
            <p class="tit">提现</p>
        </a>
    </li>
    <li class="hide">
        <a href="/Member/Referee">
            <div>
                <i class="icon icon-recommend"></i>
            </div>
            <p class="tit">推荐人</p>
        </a>
    </li>
    <li>
    <a href="/Member/IntegralAPP?type=1">
        <div>
            <i class="icon icon-fenhong"></i>
        </div>
        <p class="tit">积分兑换</p>
    </a>
    </li>
</ul>
<div class="signle-item">
    <a class="cell" href="/Member/Integral?tab=2">
        <div class="cell-left">购物积分</div>
        <div class="cell-right">
            <div class="text">&nbsp;</div>
            <i class="icon-arrow"></i>
        </div>
    </a>
    <a class="cell" href="/Member/Team">
        <div class="cell-left">我的团队</div>
        <div class="cell-right">
            <div class="text">&nbsp;</div>
            <i class="icon-arrow"></i>
        </div>
    </a>
</div>
<script>
    function Shop() {
        if (window.confirm('你确定要入住吗？必须上传商家证件才可以审核哦!')) {
            $.ajax({
                url: "/Member/OpenShop",
                data: "",
                type: "post",
                cache: false,
                timeout: 8000,
                dataType: "json",
                ContentType: "application/json; charset=utf-8",
                error: function () { alert('请求超时!'); },
                beforeSend: function () { },
                success: function (data) {
                    if (data.result === 200) {
                        self.location = "/Shop/Index";
                    }
                    else {
                        alert(data.data);
                    }
                },
            });
            return true;
        } else {
            return false;
        }
    }
    function b(account, b) {
        $.ajax({
            url: "/ajax/api.ashx?ff=mb",
            data: "account=" + account + "&b=" + b,
            type: "post",
            cache: false,
            timeout: 8000,
            dataType: "json",
            ContentType: "application/json; charset=utf-8",
            error: function () { new $.zui.Messager('请求超时!').show(); },
            beforeSend: function () { },
            success: function (data) {
                document.getElementById(b).innerHTML = data.data;
            },
        });
    }
</script>