@{
    ViewBag.Class = "Member";
    ViewBag.Title = "个人中心";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="user-sub">
    <img src="/uploadfiles/member/@ViewBag.Picture" onerror="this.src='/images/@(ViewBag.Gender+".png")'" />
    <span class="text">@ViewBag.Account (@ViewBag.RealName)</span><span class="label @(ViewBag.Project ==2?ViewBag.CLLabel:ViewBag.Label)">@(ViewBag.Project == 2 ? ViewBag.CLLevelName : ViewBag.LevelName)</span>
    <div class="level" onclick="self.location = '/Member/Info';">
        &nbsp;&nbsp;
        <i class="icon-arrow-right"></i>
    </div>
    <a class="edit" href="~/Home/LoginOut?login=/Home/Login">退出登录</a>
</div>
<div class="@(ViewBag.Level >9?"":"hide")">
    <div class="user-divider">@ViewBag.LevelName</div>
    <ul class="admin-menu">
        <li></li>
        <li>
            <a href="/Member/Profit?LevelName=@ViewBag.LevelName">
                <div>
                    <i class="icon icon-profit"></i>
                </div>
                <p class="tit">分红明细</p>
            </a>
        </li>
        <li>
            <a href="/Member/Profit">
                <div>
                    <i class="icon icon-fenhong"></i>
                </div>
                <p class="tit">占比=@ViewBag.StockRight</p>
            </a>
        </li>
        <li> </li>
    </ul>
</div>
<div class="signle-item">
    <div class="@(ViewBag.Shop !=0?"":"hide")">
        <a class="cell" href="/Shop/Index?Account=@ViewBag.Account&RealName=@ViewBag.RealName&LevelName=@ViewBag.LevelName&Label=@ViewBag.Label">
            <div class="cell-left" style="color:red;font-weight:800;padding-left:30%;">进入商家中心</div>
        </a>
    </div>
    <div class="@(ViewBag.Shop ==0?"":"hide")">
        <a class="cell" onclick="javascript:Shop();">
            <div class="cell-left" style="color:red;font-weight:800;padding-left:30%;">申请入驻成为商家</div>
        </a>
    </div>
</div>
<div class="@(ViewBag.Project ==2?"":"hide")">
    <div class="user-divider">彩链 <b style="color:cornflowerblue;font-weight:800;padding-left:8%;">当前进货价=@ViewBag.BuyPrice,库存=@ViewBag.Stock</b></div>
    <ul class="admin-menu">
        <li>
            <a href="/Member/MProduct">
                <div>
                    <i class="icon icon-goods-money"></i>
                </div>
                <p class="tit">库存提醒</p>
            </a>
        </li>
        <li>
            <a href="/Member/Product?BuyPrice=@ViewBag.BuyPrice&Type=4">
                <div>
                    <i class="icon icon-goods-money"></i>
                </div>
                <p class="tit">发货记录</p>
            </a>
        </li>
        <li>
            <a href="/Member/Product?BuyPrice=@ViewBag.BuyPrice&Type=5">
                <div>
                    <i class="icon icon-goods-money"></i>
                </div>
                <p class="tit">转售记录</p>
            </a>
        </li>
        <li>
            <a href="/Member/ProductAU?BuyPrice=@ViewBag.BuyPrice" class="@(ViewBag.SellStock ==2?"":"hide")">
                <div>
                    <i class="icon icon-storehouse"></i>
                </div>
                <p class="tit">转让商品</p>
            </a>
        </li>
        <li class="@(ViewBag.CLLevel ==26?"":"hide")">
            <a href="/Member/Bonus?Project=2">
                <div>
                    <i class="icon icon-order"></i>
                </div>
                <p class="tit">彩链分红</p>
            </a>
        </li>
    </ul>
</div>
<div>
    <div class="user-divider">钱包</div>
    <ul class="admin-menu">
        <li>
            <a onclick="b('@ViewBag.Account','BCCB');">
                <div>
                    <i class="icon icon-goods-money"></i>
                </div>
                <p class="tit">BCCB</p>
            </a>
        </li>
        <li id="BCCB"></li>
        <li>
            <a onclick="b('@ViewBag.Account','FBCC');">
                <div>
                    <i class="icon icon-goods-money"></i>
                </div>
                <p class="tit">FBCC</p>
            </a>
        </li>
        <li id="FBCC"></li>
    </ul>
</div>
<div class="@(ViewBag.Level >=6?"":"hide") @(ViewBag.Project ==1?"":"hide")">
    <div class="user-divider">合伙人</div>
    <ul class="admin-menu">
