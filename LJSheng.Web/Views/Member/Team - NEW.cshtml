@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=yes">
    <title>彩链-我的团队</title>
    <meta name="keywords" content="彩链">
    <meta name="description" content="彩链">
    <meta name="author" content="彩链">
    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
    <link rel="stylesheet" type="text/css" href="/css/style.css" />
    <link href="/plugins/mescroll/mescroll.css" rel="stylesheet" />
    <script src="/js/jquery.js"></script>
    <script src="/plugins/mlayer/layer.js"></script>
</head>
<body>
    <div class="default-header-nav">
        <span>
            <img src="/images/icon/icon-26.png" />
        </span>
        我的团队
    </div>
    <div class="team-type">
        <ul>
            <li onclick="getlist(0);">
                <span>查看</span>
                <span>全部</span>
            </li>
            <li onclick="getlist(22);">
                <span>VIP</span>
                <span>0</span>
            </li>
            <li onclick="getlist(23);">
                <span>总代</span>
                <span>0</span>
            </li>
            <li onclick="getlist(24);">
                <span>联创</span>
                <span>0</span>
            </li>
            <li onclick="getlist(25);">
                <span>合伙人</span>
                <span>0</span>
            </li>
            <li onclick="getlist(26);">
                <span>创始人</span>
                <span>0</span>
            </li>
        </ul>
    </div>
    <div id="search" hidden>
        <input type="text" name="type" id="type" value="0" />
    </div>
    <div class="mescroll" id="scroll">
        <div class="team-content" id="dataList">
            <script id="tpl" type="text/html">
                {{each list as item index}}
                <div class="team-item">
                    <div class="team-item-msg">
                        <div class="team-item-avatar">
                            <img src="/uploadfiles/member/{{item.Picture}}" onerror="this.src='/images/{{item.Gender}}.png'" />
                            <span>{{item.Number}}</span>
                        </div>
                        <div class="team-item-text">
                            <p>
                                <span class="team-item-members"><span class="label {{item.Label}}">{{item.LevelName}}</span></span>
                                <span>{{item.RealName}}</span>
                            </p>
                            <p>
                                <a class="team-item-phone">
                                    <img src="/images/icon/icon-12.png" alt="电话">
                                    <span>{{item.Account}}</span>
                                </a>
                            </p>
                        </div>
                    </div>
                    <div class="team-item-num">
                        {{item.Money}}
                    </div>
                </div>
                {{/each}}
            </script>
        </div>
    </div>
    <div class="bottom-menu">
        <div class="bottom-menu-content">
            <ul>
                <li>
                    <a href="/Home/Index" class="active">
                        <img src="images/icon/icon-42.png" class="bottom-menu-default-icon" />
                        <img src="images/icon/icon-41.png" class="bottom-menu-active-icon" />
                        <span>首页</span>
                    </a>
                </li>
                <li>
                    <a href="/Home/APP">
                        <img src="images/icon/icon-38.png" class="bottom-menu-default-icon" />
                        <img src="images/icon/icon-39.png" class="bottom-menu-active-icon" />
                        <span>APP</span>
                    </a>
                </li>
                <li>
                    <a href="/Home/Shop">
                        <img src="images/icon/icon-43.png" class="bottom-menu-default-icon" />
                        <img src="images/icon/icon-44.png" class="bottom-menu-active-icon" />
                        <span>商城</span>
                    </a>
                </li>
                <li>
                    <a href="/Member/Index">
                        <img src="images/icon/icon-45.png" class="bottom-menu-default-icon" />
                        <img src="images/icon/icon-46.png" class="bottom-menu-active-icon" />
                        <span>我的</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
    <script src="/js/template-web.js"></script>
    <script src="/plugins/mescroll/mescroll.js"></script>
    <script src="/js/mlist.js"></script>
    <script>
        function getlist(type, ev) {
            $("#type").val(type);
            $(".bonus-tabs div").removeClass("active");
            ev = ev || window.event;
            var tmp = ev.target || ev.srcElement;
            tmp.className = "active";
            //重置列表数据
            scroll.resetUpScroll();
            //隐藏回到顶部按钮
            scroll.hideTopBtn();
        }
    </script>
</body>
</html>