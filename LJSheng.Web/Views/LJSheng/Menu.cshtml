
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" Content="width=device-width" />
    <title>菜单</title>
    <link rel="stylesheet" href="~/plugins/zui/css/zui.min.css">
</head>
<body>
    <nav class="menu" data-ride="menu">
        <ul id="treeMenu" class="tree tree-menu" data-ride="tree" data-idx="0">
            <li class="active"><a href="DBBak" target="list"><i class="icon icon-th"></i>数据备份</a></li>
            <li><a href="OpenCityList" target="list"><i class="icon icon-th"></i>城市管理</a></li>
            <li class="hidden"><a href="CommentList" target="list"><i class="icon icon-th"></i>评论管理</a></li>
            <li class="has-list">
                <i class="list-toggle icon"></i>
                <a href="#"><i class="icon icon-time"></i>系统设置</a>
                <ul>
                    <li><a href="SMSList" target="list">短信管理</a></li>
                    <li><a href="DictionariesList" target="list">数据字典</a></li>
                    <li><a onclick="ExpressList();">更新快递</a></li>
                </ul>
            </li>
            <li class="has-list">
                <i class="list-toggle icon"></i>
                <a href="#"><i class="icon icon-time"></i>管理员管理</a>
                <ul>
                    <li><a href="LJShengList" target="list">管理员列表</a></li>
                    <li><a href="LJShengAU" target="list">管理员添加</a></li>
                </ul>
            </li>
            <li class="has-list">
                <i class="list-toggle icon"></i>
                <a href="#"><i class="icon icon-time"></i>会员管理</a>
                <ul>
                    <li><a href="MemberList" target="list">会员列表</a></li>
                    <li class="hidden"><a href="MemberAU" target="list">会员添加</a></li>
                </ul>
            </li>
            <li class="has-list">
                <i class="list-toggle icon"></i>
                <a href="#"><i class="icon icon-time"></i>会员等级</a>
                <ul>
                    @foreach (var item in Model)
                    {
                        <li><a href="LevelList?project=@item.Value" target="list">@(item.Key)等级列表</a></li>
                        <li><a href="LevelAU?project=@item.Value" target="list" class="hidden">@(item.Key)等级添加</a></li>
                    }
                </ul>
            </li>
            <li class="has-list">
                <i class="list-toggle icon"></i>
                <a href="#"><i class="icon icon-time"></i>分类管理</a>
                <ul>
                    @foreach (var item in Model)
                    {
                        <li><a href="ClassifyList?project=@item.Value" target="list">@(item.Key)分类列表</a></li>
                        <li><a href="ClassifyAU?project=@item.Value" target="list">@(item.Key)分类添加</a></li>
                    }
                </ul>
            </li>
            <li class="has-list hidden">
                <i class="list-toggle icon"></i>
                <a href="#"><i class="icon icon-time"></i>新闻资讯</a>
                <ul>
                    <li><a href="NewsList" target="list">新闻列表</a></li>
                    <li><a href="NewsAU" target="list">新闻添加</a></li>
                </ul>
            </li>
            <li class="has-list">
                <i class="list-toggle icon"></i>
                <a href="#"><i class="icon icon-time"></i>广告管理</a>
                <ul>
                    <li><a href="ADList" target="list">广告列表</a></li>
                    <li><a href="ADAU" target="list">广告添加</a></li>
                </ul>
            </li>
            <li class="has-list">
                <i class="list-toggle icon"></i>
                <a href="#"><i class="icon icon-time"></i>链商城商品</a>
                <ul>
                    @foreach (var item in Model)
                    {
                        <li><a href="ProductList?project=@item.Value" target="list">@(item.Key)列表</a></li>
                        <li><a href="ProductAU?project=@item.Value" target="list">@(item.Key)添加</a></li>
                        <li><a href="OrderList?project=@item.Value" target="list">@(item.Key)订单</a></li>
                    }
                    <li><a href="ConsignorList" target="list">发货人</a></li>
                    <li><a href="MProductList" target="list">库存不足</a></li>
                </ul>
            </li>
            <li class="has-list">
                <i class="list-toggle icon"></i>
                <a href="#"><i class="icon icon-time"></i>链商城管理</a>
                <ul>
                    <li><a href="OrderList" target="list">订单管理</a></li>
                    <li><a href="WithdrawalsList" target="list">提现管理</a></li>
                </ul>
            </li>
            <li class="has-list">
                <i class="list-toggle icon"></i>
                <a href="#"><i class="icon icon-time"></i>商家管理</a>
                <ul>
                    <li><a href="DFHList" target="list">待发货</a></li>
                    <li><a href="ShopList?State=2" target="list">商家列表</a></li>
                    <li><a href="ShopList?State=1" target="list">商家审核</a></li>
                    <li><a href="ShopOrderList?ReturnType=1" target="list">商家订单</a></li>
                    <li><a href="ShopOrderList?ReturnType=2" target="list">借用订单</a></li>
                    <li><a href="ShopOrderList?DFH=2" target="list">代发货订单</a></li>
                    <li><a href="ShopWithdrawalsList" target="list">商家提现</a></li>
                </ul>
            </li>
            <li class="has-list">
                <i class="list-toggle icon"></i>
                <a href="#"><i class="icon icon-time"></i>财务报表</a>
                <ul>
                    <li><a href="ReportForm" target="list">报表总汇</a></li>
                    <li><a href="OrderPay?type=1" target="list">链商城线下</a></li>
                    <li><a href="OrderPay?type=2" target="list">商城线下</a></li>
                </ul>
            </li>
            <li class="has-list">
                <i class="list-toggle icon"></i>
                <a href="#"><i class="icon icon-time"></i>分红管理</a>
                <ul>
                    <li class="hidden"><a href="StockRightList" target="list">分红项目</a></li>
                    <li class="hidden"><a href="ReportCalendar" target="list">分红管理</a></li>
                    <li class="hidden"><a href="ReportList" target="list">分红列表</a></li>
                    <li class="hidden"><a href="MReportList?auto=0" target="list">分红明细</a></li>
                    <li><a href="CLTeamList" target="list">分红管理</a></li>
                    <li class="hidden"><a href="AMList" target="list">最后一级</a></li>
                </ul>
            </li>
            <li class="has-list">
                <i class="list-toggle icon"></i>
                <a href="#"><i class="icon icon-time"></i>资金记录</a>
                <ul>
                    <li><a href="IntegralList" target="list">积分记录</a></li>
                    <li><a href="ShopRecordList" target="list">商城积分</a></li>
                    <li><a href="CLRecordList" target="list">额度兑换</a></li>
                    <li><a href="FrozenIntegralList" target="list">冻结记录</a></li>
                    <li><a href="TokenRecordList" target="list">兑换记录</a></li>
                </ul>
            </li>
        </ul>
    </nav>

    <script src="~/plugins/zui/lib/jquery/jquery.js"></script>
    <script src="~/plugins/zui/js/zui.min.js"></script>
    <script>
        $('#treeMenu').on('click', 'a', function () {
            $('#treeMenu li.active').removeClass('active');
            $(this).closest('li').addClass('active');
        });
        function ExpressList() {
            if (window.confirm('你确定要执行此操作吗？')) {
                $.ajax({
                    url: "/LJSheng/ExpressList",
                    data: "",
                    type: "post",
                    cache: false,
                    timeout: 8000,
                    dataType: "json",
                    ContentType: "application/json; charset=utf-8",
                    error: function () { },
                    beforeSend: function () { },
                    success: function (data) {
                        new $.zui.Messager(data.data, {
                            icon: data.result == 200 ? 'smile' : 'frown',
                            placement: 'center'
                        }).show();
                    },
                });
                return true;
            } else {
                return false;
            }
        }
    </script>
</body>
</html>