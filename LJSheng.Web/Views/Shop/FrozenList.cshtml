@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=yes">
    <title>彩链-冻结记录</title>
    <meta name="keywords" content="彩链">
    <meta name="description" content="彩链">
    <meta name="author" content="彩链">
    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
    <link rel="stylesheet" type="text/css" href="/css/style.css" />
    <link href="/plugins/mescroll/mescroll.css" rel="stylesheet" />
</head>

<body>
    <div class="default-header-nav">
        <span>
            <a href="/Member/MyShop">
                <img src="/images/icon/icon-26.png" />
            </a>
        </span> 冻结记录
    </div>
    <div id="search" hidden>
        <input type="text" name="type" value="@Request.QueryString["type"]" />
    </div>
    <div class="mescroll" id="scroll">
        <div class="frozen-record" id="dataList">
            <script id="tpl" type="text/html">
                {{each list as item index}}
                <div class="frozen-record-item">
                    <div class="frozen-record-item-type">
                        <p>商城积分</p>
                        <span>冻结日期：{{item.AddTime}}</span>
                        <span>解冻日期：{{item.ThawTime}}</span>
                    </div>
                    <div class="frozen-record-item-num positive-num">
                        {{item.TIntegral}}
                    </div>
                </div>
                {{/each}}
            </script>
        </div>
    </div>
    <script src="/js/jquery.js"></script>
    <script src="/js/template-web.js"></script>
    <script src="/plugins/mescroll/mescroll.js"></script>
    <script src="/js/mlist.js"></script>
</body>
</html>